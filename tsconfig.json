{
    "compilerOptions": {
        "target": "es2020",
        "module": "commonjs",
        "moduleResolution": "node",
        "lib": [
            "es2021"
        ],
        "types": [
            "@types/node",
            "@citizenfx/server",
            "@citizenfx/client"
        ],
        "declaration": true,
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "strictNullChecks": true,
        "skipLibCheck": true
    }
}